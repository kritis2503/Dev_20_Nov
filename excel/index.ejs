<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Excel</title>
        <link rel="stylesheet" href="./css/style.css">
    </head>
    <body>

        <div class="menu">
            <div class="file active">File</div>
            <div class="home">Home</div>
        </div>
        <div class="menu-option">
            <div class="file-menu-options">
                <div class="new">New</div>
                <div class="open">Open</div>
                <div class="save">Save</div>
            </div>
            <div class="home-menu-options hide">
                Home Menu Options
            </div>
        </div>
        <div class="formula">
            <input type="text" id="address" disabled>
          <input type="text" id="formula">
        </div>
        <div class="content">
            <div class="top-left-cell">

            </div>
            <div class="top-row">
                <%for(let j=0 ; j<26 ; j++){%>
                    <div class="top-row-cell" cellid = <%= j%>> <%= String.fromCharCode(65+j) %>    </div>
                    <%}%>              
            </div>
            <div class="left-col">
                <%for(let j=0 ; j<100 ; j++){%>
                    <div class="left-col-cell" cellid = <%= j%>> <%= j+1 %>    </div>
                    <%}%> 
            </div>
        <div class="cells">
            <%for(let i=0;i<100;i++) {%>
                <div class="row">
                    <% for(let j=0;j<26;j++){ %>
                        <div class="cell" contenteditable="true" rowid=<%=i%> colid=<%=j%> ></div>
                        <%}%>
                </div>
                <%}%>
        </div>
        </div>
        <div class="sheet">sheet</div>
         <script src="./js/script.js"></script> 
    </body>
</html>